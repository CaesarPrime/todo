<table class="environment-variables" border="1">
  <thead>
    <tr>
      <th class="name">Name</th>
      <th class="value">Value</th>
    </tr>
  </thead>
  <tbody>
  <% ENV.sort.each do |name, value| %>
    <tr>
      <td><strong><%= name %></strong></td>
      <td><%= value %></td>
    </tr>
  <% end %>
  <tr>
      <td><strong>which gem</strong></td>
      <td><%= system("which gem") %></td>
    </tr>
  </tbody>
</table>
